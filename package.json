{
  "name": "Michaeng",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint",
    "reinstall": "watchman watch-del-all && rm -rf node_modules && yarn cache clean && yarn install",
    "clear-cache": "rm -rf $TMPDIR/react-*",
    "clear": "rm -rf $TMPDIR/haste-map-react-native-packager-* && rm -rf /tmp/metro-bundler-cache-*",
    "clear-all": "rm -rf $TMPDIR/haste-map-react-native-packager-* && rm -rf /tmp/metro-bundler-cache-* && rm -rf $TMPDIR/react-*",
    "restart": "watchman watch-del-all && react-native start -- --reset-cache",
    "android": "react-native run-android",
    "android-release": "react-native run-android --variant=release",
    "bundle-android-assets": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "clean-android": "cd android && ./gradlew clean && cd ..",
    "ios": "react-native run-ios",
    "ios-5": "react-native run-ios --simulator 'iPhone 5s'",
    "ios-6s": "react-native run-ios --simulator 'iPhone 6s'",
    "ios-8": "react-native run-ios --simulator 'iPhone 8 Plus'",
    "ios-x": "react-native run-ios --simulator 'iPhone X'",
    "ios-xs": "react-native run-ios --simulator='iPhone Xs'",
    "install-pods": "cd ios && pod install && cd ..",
    "postinstall": "npx jetify"
  },
  "dependencies": {
    "formik": "^1.5.8",
    "prop-types": "^15.7.2",
    "react": "16.8.6",
    "react-native": "0.60.4",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-indicator": "^1.0.0",
    "react-native-keyboard-aware-scroll-view": "^0.8.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^3.11.1",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-saga": "^1.0.5",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.14.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "^0.55.0",
    "react-test-renderer": "16.8.6"
  },
  "jest": {
    "preset": "react-native"
  }
}
